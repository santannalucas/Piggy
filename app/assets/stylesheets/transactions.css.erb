.transaction{
    border:solid 1px #d3d3d3;
    border-radius: 15px;
    height: 50px;
    padding: 10px;
    margin: 5px;
}

.trans_account{
    float: left;
    width: 600px;
    text-align: left;
    font-size: 19px;
    font-weight: bold;
    margin: 0 0 10px;
}

.trans_category{
    float: left;
    width: 600px;
    text-align: left;
    font-size: 16px;
    color: #838383;
}

.trans_amount{
    float:right;
    font-size: 18px; !important;
}
.trans_date{
    text-align: right;
    width: 170px;
    height: 25px;
    float:right;
    font-size: 15px;
}











