<!-- Title and Hide Search Toggle -->
<% content_for :title, "#{@import.name} File" %>


<div id='workspace'>
  <h1><%= @import.name %></h1>
  <br>
  <%= form_for new_import_file_path(id: @import.id), multipart:true, method: 'get' do %>
    <%= file_field_tag :file, class: 'input' %>
    <%= hidden_field_tag :id, @import.id %>
  <%= submit_tag 'Import' %>
  <% end %>

</div>

<!-- Bottom Menu -->
<div id="menu-bottom">
  <ul>
    <!-- Back Dashboard -->  <li> <%= link_to root_path do %><i class="fad fa-analytics fa-3x dashboard" title="Back to Dashboard"></i><br>Dashboard<% end %></li>
  </ul>
</div>